{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "dist-server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "dist-server/index.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}