{
    "version": 2,
    "builds": [
        {
            "src": "dist-server/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "dist-server/index.js"
        },
        {
            "src": "^/assets/(.*)",
            "dest": "dist/assets/$1"
        },
        {
            "src": "^/favicon.ico",
            "dest": "dist/favicon.ico"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "dist/index.html"
        }
    ]
}